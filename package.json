{
	"$schema": "https://json.schemastore.org/package",
	"private": true,
	"version": "0.0.0",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"benchmark": "turbo benchmark",
		"build": "turbo build",
		"check": "run-s --continue-on-error check:*",
		"check:prettier": "prettier --check .",
		"check:rome": "rome check .",
		"clean": "turbo clean",
		"fix": "run-s fix:*",
		"fix:prettier": "prettier --write .",
		"fix:rome": "rome check --apply .",
		"test": "turbo test",
		"watch": "turbo watch"
	},
	"devDependencies": {
		"@types/node": "^20.4.0",
		"npm-run-all2": "^6.0.5",
		"prettier": "^3.0.0",
		"prettier-plugin-organize-imports": "^3.2.2",
		"rimraf": "^5.0.1",
		"rome": "^12.1.3",
		"turbo": "^1.10.7",
		"typescript": "^5.1.6"
	}
}
